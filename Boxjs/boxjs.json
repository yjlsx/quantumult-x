   {
     "id": "QQreader",
     "name": "QQ阅读",
     "keys": ["qqrdcookie"],
     "author": "@yjlsx",
     "repo": "https://raw.githubusercontent.com/yjlsx/quantumult-x/master/Task",
     "icons": ["https://raw.githubusercontent.com/Koolson/Qure/master/IconSet/Color/QQ.png"],
