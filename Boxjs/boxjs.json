   {
     "id": "NeteaseMusic",
     "name": "网易云音乐",
     "author": "@yjlsx",
     "icons": ["https://raw.githubusercontent.com/yjlsx/quantumult-x/master/IconSet/Color/wyy.png"],
     "script": "https://raw.githubusercontent.com/yjlsx/quantumult-x/master/Task/wyyyy.js",
      "keys": [
        "wyyyy_data",
        "enable_signin",
        "enable_yunbei",
        "enable_shuffle",
        "song_limit"
      ],
      "boxjs": {
        "schema": {
          "wyyyy_data": {
            "type": "textarea",
            "label": "账号Cookie",
            "desc": "多账号用 & 分隔",
            "input": "text",
            "rows": 3,
            "placeholder": "MUSIC_U=xxx; __csrf=xxx;"
          },
          "enable_signin": {
            "type": "boolean",
            "label": "启用签到",
            "desc": "每日自动签到开关",
            "default": true
          },
          "enable_yunbei": {
            "type": "boolean",
            "label": "云贝签到",
            "desc": "云贝自动签到开关",
            "default": true
          },
          "enable_shuffle": {
            "type": "boolean",
            "label": "刷歌功能",
            "desc": "自动刷歌开关",
            "default": true
          },
          "song_limit": {
            "type": "number",
            "label": "刷歌数量",
            "desc": "默认10首",
            "default": 10,
            "min": 5,
            "max": 30
          }
        }
      }
    },
   {
