 {
  "id": "kuaishou",
  "name": "快手签到",
  "author": "@yjlsx",
  "repo": "https://raw.githubusercontent.com/yjlsx/quantumult-x/master/Task",
  "icons": ["https://raw.githubusercontent.com/yjlsx/quantumult-x/master/IconSet/Color/kuaishou.png"],
  "script": "https://raw.githubusercontent.com/yjlsx/quantumult-x/master/Task/kuaishou1.js",
  "keys": ["ks_enabled_1", "ks_cookie_1", "ks_enabled_2", "ks_cookie_2"],
  "functions": [
    {
      "id": "ks_enabled_1",
      "title": "启用账号1",
      "desc": "是否启用账号1",
      "icon": "ks",
      "config": {
        "name": "ks_enabled_1",
        "type": "boolean",
        "default": true
      }
    },
    {
      "id": "ks_cookie_1",
      "title": "账号1 Cookie",
      "desc": "用于快手签到",
      "icon": "ks",
      "config": {
        "name": "ks_cookie_1",
        "type": "text",
        "input": "password",
        "readonly": false
      }
    },
    {
      "id": "ks_enabled_2",
      "title": "启用账号2",
      "desc": "是否启用账号2",
      "icon": "ks",
      "config": {
        "name": "ks_enabled_2",
        "type": "boolean",
        "default": true
      }
    },
    {
      "id": "ks_cookie_2",
      "title": "账号2 Cookie",
      "desc": "用于快手签到",
      "icon": "ks",
      "config": {
        "name": "ks_cookie_2",
        "type": "text",
        "input": "password",
        "readonly": false
      }
    }
  ],
"boxjs": {
  "hosts": ["kuaishou.com"],
  "schema": {
    "ks_enabled_1": {
      "type": "boolean",
      "label": "启用账号1",
      "desc": "控制是否启用快手账号1",
      "default": true,
      "val": true  // 这里加上 "val" 赋值，确保默认值生效
    },
    "ks_cookie_1": {
      "type": "text",
      "label": "账号1 Cookie",
      "desc": "用于快手签到",
      "input": "password"
    },
    "ks_enabled_2": {
      "type": "boolean",
      "label": "启用账号2",
      "desc": "控制是否启用快手账号2",
      "default": true,
      "val": true
    },
    "ks_cookie_2": {
      "type": "text",
      "label": "账号2 Cookie",
      "desc": "用于快手签到",
      "input": "password"
       }
    }
  }
